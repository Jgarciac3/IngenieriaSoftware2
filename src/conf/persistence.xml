<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="SIGAPU" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>jdbc/siga</jta-data-source>
    <class>siga.persistence.entities.AdmPersonas</class>
    <class>siga.persistence.entities.Acudiente</class>
    <class>siga.persistence.entities.AcudienteAspirante</class>
    <class>siga.persistence.entities.Anios</class>
    <class>siga.persistence.entities.Aspirante</class>
    <class>siga.persistence.entities.Departamentos</class>
    <class>siga.persistence.entities.InfoAcademica</class>
    <class>siga.persistence.entities.Inscripcion</class>
    <class>siga.persistence.entities.Municipios</class>
    <class>siga.persistence.entities.Roles</class>
    <class>siga.persistence.entities.TipoDocumento</class>
    <class>siga.persistence.entities.UsuarioRol</class>
    <class>siga.persistence.entities.Usuarios</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
